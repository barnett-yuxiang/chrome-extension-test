{
  "manifest_version": 3,
  "name": "My Service Worker Test Extension",
  "version": "1.0.1",
  "description": "A test extension to try Service Worker functionality",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  }
}
